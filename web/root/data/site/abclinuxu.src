# ABCLinuxu search plug-in
# info #at# abclinuxu.cz
# $Id$

<search
   name="Portal ABC Linuxu"
   description="www.abclinuxu.cz - tady je tucnakum hej"
   method="GET"
   action="http://www.abclinuxu.cz/Search"
   searchForm="http://www.abclinuxu.cz/Search"
   queryEncoding="iso-8859-2"
   queryCharset="iso-8859-2"
>

<input name="query" user>
<input name="sourceid" value="mozilla-search">

<interpret
   browserResultType="result"
   charset="iso-8859-2"
   resultItemStart="<!--m-->"
   resultItemEnd="<!--n-->"
>

</search>

<browser
   update="http://www.abclinuxu.cz/data/site/abclinuxu.src"
   updateIcon="http://www.abclinuxu.cz/images/site/abclinuxu.png"
   updateCheckDays="3"
>
