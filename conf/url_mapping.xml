    <deprecated version="www">
        <mapping>
            <pattern input="host">^@DOMAIN_NAME@</pattern>
            <regexp>://</regexp>
            <replacement>://www.</replacement>
        </mapping>
        <mapping>
            <pattern input="host">abclinux.cz</pattern>
            <regexp>.*</regexp>
            <replacement>http://www.abclinuxu.cz/</replacement>
        </mapping>
        <mapping>
            <pattern input="host">abclinuxu.sk</pattern>
            <regexp>abclinuxu.sk</regexp>
            <replacement>abclinuxu.cz</replacement>
        </mapping>
        <mapping>
            <pattern input="host">palirna</pattern>
            <regexp>.*</regexp>
            <replacement>http://www.abclinuxu.cz/</replacement>
        </mapping>
        <mapping>
            <pattern input="path">^/news/</pattern>
            <regexp>/news/</regexp>
            <replacement>/zpravicky/</replacement>
        </mapping>
        <mapping>
            <pattern input="path">^/drivers</pattern>
            <regexp>/drivers</regexp>
            <replacement>/ovladace</replacement>
        </mapping>
        <mapping>
            <pattern input="path">ViewRelation</pattern>
            <regexp>ViewRelation.+(relationId|rid)=([\d]+).*</regexp>
            <replacement>show/$2</replacement>
        </mapping>
        <mapping>
            <pattern input="path">ViewCategory</pattern>
            <regexp>ViewCategory.+(relationId|rid|categoryId)=([\d]+).*</regexp>
            <replacement>dir/$2</replacement>
        </mapping>
        <mapping>
            <pattern input="path">Show</pattern>
            <regexp>Show\?(relationId|rid)=</regexp>
            <replacement>dir/</replacement>
        </mapping>
        <mapping>
            <pattern input="path">/EditItem</pattern>
            <regexp>/EditItem</regexp>
            <replacement>/edit</replacement>
        </mapping>
    </deprecated>
</mappings>
